package school;

public class Teacher extends Subject {
  private String teacherName = "";
  private boolean presence = true;
  
  
  
  public Teacher(String subjectName, String time, int roomNumber, int session, String teacherName, boolean presence) {
	  super(subjectName, time, roomNumber, session);
	  this.teacherName = teacherName;
	  this.presence = presence;
  }
  


public void setTeacherName(String teacherName){
		this.teacherName = teacherName;
	}
  
  public void setPresence(boolean presence){
		this.presence = presence;
	}
  
  public String getTeacherName(){
		return this.teacherName;
	}
  
  public boolean presence(){
		return this.presence;
	}
}
