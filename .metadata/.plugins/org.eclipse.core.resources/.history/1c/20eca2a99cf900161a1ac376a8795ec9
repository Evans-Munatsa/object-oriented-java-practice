package school;

public class main {

	public static void main(String[] args) {
		Subject period1 = new Subject();
		System.out.println(period1.getSubjectName());
		period1.setName("Maths");
		System.out.println(period1.getSubjectName());
		
		Subject period2 = new Subject("English", "11:00", 10, 2);
		System.out.println(period2.getSubjectName());
		System.out.println(period2.getRoomNumber());
	}

}
