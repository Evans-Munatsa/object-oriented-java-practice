package school;

import school.Teacher;

public class main {

	public static void main(String[] args) {
		
		Teacher period1 = new Teacher("Subject: Maths", "Time: 10:10", 6, 3, "Teacher: Mr Evans Munatsa", true, 20);
		System.out.println(period1.getTeacherName());
		System.out.println(period1.getPresence());
		
		Student student1 = new Student("Maths", "10:10", 6, 3, "Student: Miriam Munatsa", "12B", true);
		System.out.println(student1.getStudentName());
	}

}
